<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <package name="lwbSpread" extends="json-default">
        <action name="lwb_spread_list" class="lwbSpreadAction" method="list">
            <result type="json">
                <param name="root"> map</param>
            </result>
        </action>
        <action name="lwb_spread_add" class="lwbSpreadAction" method="add">
            <result type="json">
                <param name="root">errorMsg</param>
            </result>
        </action>
        <action name="lwb_spread_delete" class="lwbSpreadAction" method="delete">
            <result type="json">
                <param name="root">errorMsg</param>
            </result>
        </action>
    </package>
</struts>